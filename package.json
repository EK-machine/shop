    "prepare": "husky install",
    "mock-server": "json-server src/mockserver/mockserver.json --watch -p 3001",
    "error-server": "json-server src/errorserver/errorserver.json --watch -p 3002",
    "serve": "serve \"./dist\" -s",
    "analyze": "webpack-bundle-analyzer \"./dist/stats.json\""
